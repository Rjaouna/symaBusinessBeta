{% extends 'base.html.twig' %}

{% block title %}CarteSim{% endblock %}

{% block body %}
    <h1>CarteSim</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ carte_sim.id }}</td>
            </tr>
            <tr>
                <th>SerialNumber</th>
                <td>{{ carte_sim.serialNumber }}</td>
            </tr>
            <tr>
                <th>Reserved</th>
                <td>{{ carte_sim.reserved ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ carte_sim.createdAt ? carte_sim.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ carte_sim.updatedAt ? carte_sim.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_carte_sim_index') }}">back to list</a>

    <a href="{{ path('app_carte_sim_edit', {'id': carte_sim.id}) }}">edit</a>

    {{ include('carte_sim/_delete_form.html.twig') }}
{% endblock %}
