{% extends 'base.html.twig' %}

{% block title %}Quota index{% endblock %}

{% block body %}
    <h1>Quota index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Sim5Quota</th>
                <th>Sim10Quota</th>
                <th>Sim15Quota</th>
                <th>Sim20Quota</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for quotum in quotas %}
            <tr>
                <td>{{ quotum.id }}</td>
                <td>{{ quotum.nom }}</td>
                <td>{{ quotum.sim5Quota }}</td>
                <td>{{ quotum.sim10Quota }}</td>
                <td>{{ quotum.sim15Quota }}</td>
                <td>{{ quotum.sim20Quota }}</td>
                <td>{{ quotum.createdAt ? quotum.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ quotum.updatedAt ? quotum.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_quota_show', {'id': quotum.id}) }}">show</a>
                    <a href="{{ path('app_quota_edit', {'id': quotum.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_quota_new') }}">Create new</a>
{% endblock %}
